{
  "name": "frontend_env",
  "version": "1.0.0",
  "description": "FrontEndEnv based on webpack",
  "main": "index.js",
  "author": "Koudai ishigame",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"src/**/*.js\"",
    "lint-fix": "eslint \"src/**/*.js\" --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --config ./webpack/dev.config.ts --hot",
    "dev:gen": "npm run build && cd dist && python -m SimpleHTTPServer",
    "build": "rm -rf ./dist && webpack --mode production --config ./webpack/prod.config.ts",
    "start": "webpack-dev-server --config ./webpack/prod.config.ts --host 0.0.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.5.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "html-webpack-plugin": "^4.0.4",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.4",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "thread-loader": "^2.1.3",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  },
  "repository": "https://github.com/isihigameKoudai/FrontendEnv.git",
  "dependencies": {
    "@types/webpack": "^4.41.6",
    "glob": "^7.1.6",
    "node-sass": "^4.13.1",
    "ts-node": "^8.6.2"
  }
}
