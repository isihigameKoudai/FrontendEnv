{
  "name": "frontend_env",
  "version": "1.0.0",
  "description": "FrontEndEnv based on webpack",
  "main": "index.js",
  "author": "Koudai ishigame",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"src/**/*.js\"",
    "lint-fix": "eslint \"src/**/*.js\" --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --config ./webpack/dev.config.ts --hot",
    "dev:gen": "npm run build && cd dist && python -m SimpleHTTPServer",
    "build": "rm -rf ./dist && webpack --mode production --config ./webpack/prod.config.ts",
    "start": "webpack-dev-server --config ./webpack/prod.config.ts --host 0.0.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "cache-loader": "^4.1.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "html-webpack-plugin": "^4.5.1",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "thread-loader": "^3.0.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^3.0.0",
    "webpack-merge": "^5.7.3"
  },
  "repository": "https://github.com/isihigameKoudai/FrontendEnv.git",
  "dependencies": {
    "glob": "^7.1.6",
    "node-sass": "^5.0.0",
    "ts-node": "^9.1.1"
  }
}
