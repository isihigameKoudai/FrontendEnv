{
  "name": "frontend_env",
  "version": "1.0.0",
  "description": "FrontEndEnv based on webpack",
  "main": "index.js",
  "author": "Koudai ishigame",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"src/**/*.js\"",
    "lint-fix": "eslint \"src/**/*.js\" --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --config ./webpack/dev.config.ts --hot",
    "dev:gen": "npm run build && cd dist && python -m SimpleHTTPServer",
    "build": "rm -rf ./dist && webpack --mode production --config ./webpack/prod.config.ts",
    "start": "webpack-dev-server --config ./webpack/prod.config.ts --host 0.0.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.1",
    "cache-loader": "^4.1.0",
    "css-loader": "^1.0.1",
    "dart-sass": "^1.25.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.11.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.23.1",
    "thread-loader": "^2.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "url-loader": "^2.3.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.1.1",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.1.3"
  },
  "repository": "https://github.com/isihigameKoudai/FrontendEnv.git",
  "dependencies": {
    "@types/webpack": "^4.41.6",
    "node-sass": "^4.13.1",
    "ts-node": "^8.6.2"
  }
}
